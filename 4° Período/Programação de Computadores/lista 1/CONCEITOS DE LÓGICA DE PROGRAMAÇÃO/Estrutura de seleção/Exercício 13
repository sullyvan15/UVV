#include <stdio.h>

#include <math.h>

int main()
{
    int num1, num2, num3;

    printf("Digite o primeiro numero: ");
    scanf_s("%d", & num1);
    printf("Digite o segundo numero: ");
    scanf_s("%d", & num2);
    printf("Digite o terceiro numero: ");
    scanf_s("%d", & num3);
    int media = (num1 + num2 + num3) / 3;
    double cubo = pow(media, 3);
    double
    if ((media > 10) && (media < 200))
        printf (" O Cubo deste intervalo e %f\n", cubo);
    else
        printf (" A media deste intervalo e %d\n", media);

}
